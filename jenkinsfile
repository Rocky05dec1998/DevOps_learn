pipeline {
agent any 
stages{
stage('test') {
steps{
sh 'echo "helllo jenkins"'
echo '2nd hello'
}
}
stage('server-login') {
            steps {
                withCredentials([sshUserPrivateKey(credentialsId: 'devops-ssh', keyFileVariable: 'KEY_FILE')]) {
                     sh "ssh -o StrictHostKeyChecking=no -i \$KEY_FILE jenkins@172.31.26.104 
                        'echo hello world >> test2.txt'
                      "
                }
            }
}
}
}
