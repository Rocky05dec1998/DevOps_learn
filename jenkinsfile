pipeline {
agent any 
stages{
stage('test') {
steps{
sh 'echo "helllo jenkins"'
echo '2nd hello'
}
}
stage('server-login') {
steps{
  sshagent (credentials: ['devops-ssh']) {
    sh '''
       ssh -o StrictHostKeyChecking=no jenkins@172.31.26.104
       echo 'hello world'>>devopse.txt
       sudo yum install -y nginx
       sudo systemctl enable nginx
       sudo systemctl start nginx
       sudo systemctl status nginx
   '''
  }
}
}

}
}
